{
  "name": "barrios-landing",
  "version": "2.0.0",
  "description": "Barrios A2I - Intelligence Architecture",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "stripe:seed": "node scripts/seed-stripe-products.js",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "css:build": "npx @tailwindcss/cli -i ./src/input.css -o ./dist/output.css --minify",
    "css:watch": "npx @tailwindcss/cli -i ./src/input.css -o ./dist/output.css --watch",
    "css:sync": "cp dist/output.css public/dist/output.css",
    "dev:full": "npm run css:build && npm run css:sync && concurrently \"npm run css:watch\" \"npm run dev\" --names \"CSS,NEXT\" --prefix-colors \"cyan,magenta\"",
    "preview": "npm run css:build && npm run css:sync && npm run dev",
    "preview:static": "npm run css:build && npm run css:sync && npx serve . -p 5000",
    "prebuild": "npm run css:build && npm run css:sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BarriosA2I/barrios-landing.git"
  },
  "keywords": [
    "AI",
    "automation",
    "enterprise"
  ],
  "author": "Gary Barrios",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BarriosA2I/barrios-landing/issues"
  },
  "homepage": "https://github.com/BarriosA2I/barrios-landing#readme",
  "dependencies": {
    "@clerk/nextjs": "^6.36.8",
    "@clerk/themes": "^2.4.48",
    "@prisma/client": "^5.22.0",
    "@react-email/components": "^1.0.6",
    "@stripe/stripe-js": "^8.6.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.26.2",
    "lucide-react": "^0.454.0",
    "next": "^14.2.15",
    "prisma": "^5.22.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^6.8.0",
    "stripe": "^20.2.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@react-email/preview-server": "^5.2.5",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.2",
    "dotenv-cli": "^11.0.0",
    "postcss": "^8.5.6",
    "react-email": "^5.2.5",
    "serve": "^14.2.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.3"
  }
}
